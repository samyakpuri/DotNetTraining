<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttp" />
                <binding name="BasicHttp_IAdvance" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttp" />
                <binding name="WSHttp_IAdvance" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:54502/CalaculatorService.svc/basic"
                binding="basicHttpBinding" bindingConfiguration="BasicHttp"
                contract="Calculator.ICalculator" name="BasicHttp" />
            <endpoint address="http://localhost:54502/CalaculatorService.svc/ws"
                binding="wsHttpBinding" bindingConfiguration="WSHttp" contract="Calculator.ICalculator"
                name="WSHttp">
                <identity>
                    <userPrincipalName value="spuri4@sapient.com" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:54502/CalaculatorService.svc/basic2"
                binding="basicHttpBinding" bindingConfiguration="BasicHttp_IAdvance"
                contract="Calculator.IAdvanceCalculator" name="BasicHttp_IAdvance" />
            <endpoint address="http://localhost:54502/CalaculatorService.svc/ws2"
                binding="wsHttpBinding" bindingConfiguration="WSHttp_IAdvance"
                contract="Calculator.IAdvanceCalculator" name="WSHttp_IAdvance">
                <identity>
                    <userPrincipalName value="spuri4@sapient.com" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>